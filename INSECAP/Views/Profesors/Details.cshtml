@model INSECAP.Models.Profesor

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles del Profesor</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            RUT DEL PROFESOR
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RunProfesor)-@Html.DisplayFor(model => model.DvProfesor)
        </dd>
        <dt class = "col-sm-2">
            NOMBRE DEL PROFESOR
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre) @Html.DisplayFor(model => model.Apellido)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.RunProfesor" class="btn btn-info">Editar</a> |
    <a asp-action="Index" class="btn btn-dark">Volver al Listado</a>
</div>
