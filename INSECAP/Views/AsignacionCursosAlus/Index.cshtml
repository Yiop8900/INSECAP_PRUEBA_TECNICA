@model IEnumerable<INSECAP.Models.AsignacionCursosAlu>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CodigoCursoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodigoSalaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdBimestreNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RunAlumnoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CodigoCursoNavigation.CodigoCurso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodigoSalaNavigation.CodigoSala)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdBimestreNavigation.IdBimestre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RunAlumnoNavigation.RunAlumno)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAsignacion">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAsignacion">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAsignacion">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
